{
  "AP_SSID":  "ESP8266",
   "AP_PSK": "12345678",
 "STA_SSID":   "MYHOME",
  "STA_PSK": "12345678",
  "BlinKey": "",
	    "ss": 4,
	 "Servo": [ "X",  "Y",  "Z",  "E",  "B",  "T"],
	   "pin": [   4,    0,   16,   15,   14,  12 ],
	"rawdms": [1500, 1500, 2500, 1500, 1500, 1500],
	"mindms": [ 500,  500, 1500, 1050,  500,  500],
	"maxdms": [2500, 2500, 2500, 1720, 2500, 2500]
}
//固件参数配置文件.修改内容上传到开发板就能替换掉对应变量
//AP是要创建的WIFI,STA是要连接的路由器
//BlinKey为物连网 点灯科技 APP里创建的独立设备密钥
//ss=4为四轴,ss=5为五轴,ss=6或其他值为六轴机械臂
//Servo        各舵机名称编号
//pin          舵机黄线信号连到板子的 GPIO
//rawdms       各舵机原点位置脉宽
//mindms       各舵机最小角度脉宽
//maxdms       各舵机最大角度脉宽